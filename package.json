{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.11.0",
    "prettier": "1.16.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "loaders.css": "^0.1.2",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "pubsub-js": "^1.7.0",
    "react": "^16.7.0",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-transition-group": "^2.5.3",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0",
    "reselect": "^4.0.0",
    "validator": "^10.11.0"
  },
  "scripts": {
    "start": "PORT=4200 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "react/prop-types": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "no-console": 1,
      "max-len": [
        "error",
        {
          "code": 160
        }
      ]
    },
    "plugins": [
      "react",
      "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
